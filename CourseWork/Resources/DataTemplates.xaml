<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CourseWork.ViewModels" xmlns:local1="clr-namespace:CourseWork.Views"
                    xmlns:admin="clr-namespace:CourseWork.ViewModels.AdminViewModels"
                    xmlns:adminV="clr-namespace:CourseWork.Views.AdminViews">
    <DataTemplate DataType="{x:Type local:SearchViewModel}">
        <local1:SearchView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:HomeViewModel}">
        <local1:HomeView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:CartViewModel}">
