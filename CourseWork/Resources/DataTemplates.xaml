        <local1:LoginView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:RegViewModel}">
        <local1:RegView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:ConfirmOrderViewModel}">
        <local1:ConfirmOrderView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type admin:HomeAdminVM}">
        <adminV:AdminHomeView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type admin:UsersAdminVM}">
        <adminV:AdminUsersView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type admin:OrdersAdminVM}">
        <adminV:OrdersAdminView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:AboutViewModel}">
        <local1:AboutView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:ChangePasswordViewModel}">
        <local1:ChangePasswordView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:CancelOrderViewModel}">
